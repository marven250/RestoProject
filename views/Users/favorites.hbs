<h1 style="text-align: center">Personal Favorites</h1>
<div class="menu">
  {{#each food}}
  <div class="foodItem">
    <span class="title"><a class="titles">{{this.title}}</a></span>
    <br>
    <div class="img"><img class="src" src='{{this.image}}'>
      <div class="text"><a>{{this.type}}</a></div>
      <div class="text"><a>{{this.description}}</a></div>
    </div>
    <br><br>
  </div>
  {{/each}}
</div>
